<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <name>AV-media app</name>
    <description>
        Skanna besökare och håll koll på dina kontaker.
    </description>
    <author email="jcbpettersson@gmail.com" href="none">
        TuT Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/img/icon.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="orientation" value="portrait" />
</widget>
